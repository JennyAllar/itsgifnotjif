

<!DOCTYPE html>
<html>
<head>
  <title>It's Gif, Not Jif</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href='http://fonts.googleapis.com/css?family=Amatic+SC|Oswald:300,400|Open+Sans+Condensed:300,700|Nunito' rel='stylesheet' type='text/css'>  <!--[if IE]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
  <link rel="stylesheet" type="text/css" href="../css/731438.css" />
</head>
<body>
<html><head>
  <meta charset="UTF-8">

  <title>It's Gif, Not Jif</title>

  <link href="style.css" rel="stylesheet" type="text/css">

  <link rel="stylesheet" type="text/css" href="http://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.1/normalize.min.css">

</head>

<body>

<header>
  <div id="header_container">
    <div id="header_logo"><%= link_to image_tag("logo.gif"), root_path %>
    </div><!--end header_logo-->
  </div><!--end header-->
</header>

<nav>
  <div id="nav_container">

    <ul id="menu">
      <li><a href="/">HOME</a></li>
      <li><a href="/favorites"><%= image_tag("heart.png", height: '15', width: '10') %>&nbsp;&nbsp;&nbsp;&nbsp;LIKES</a></li>
      <li><a href="/random">RANDOM</a></li>
      <li><a href="/jif">JIF? NO.</a></li>
      <li><a href="/about">ABOUT ME</a></li>

    </ul>
<ul id="search_bar">
  <%= form_tag gifs_path, :method => 'get' do %>
      <%= text_field_tag :search, "Search Gifs" %>
     <%= image_submit_tag("icon_search.png", height: '30') %>
  <% end %>

  </ul>
  </div><!--end navigation-->
</nav>

<div id="content">
  <div id="content_container">

    <div id="columns_2">

      <header><% if @user %>
          <%= button_to 'Add a Gif!', new_gif_path, method: "get", class: "button-2" %>
          <%= button_to 'Sign Out', logout_path, action: :delete, method: "get", class: "button-2" %>
        <% else %>
          <%= button_to 'Sign In', login_path, method: "get", class: "button-2" %>
          <%= button_to 'Register Here!', new_user_path, method: "get", class: "button-2" %>
          <%= button_to 'Add a Gif!', new_gif_path, method: "get", class: "button-2" %>
        <% end %>
      </header>
      <br>
      <br>
      <br>
    </div>
<%= yield %>

</div>
</div><!--end content-->

<footer>
  <div id="footer_container">
    <ul id="menu_footer">
      <li><a href="https://twitter.com/JennyAllar" target="_blank"><%= image_tag("twitter.png", height: '40') %>&nbsp;&nbsp;&nbsp;&nbsp;FOLLOW ME ON TWITTER</a></li>
  </ul>
    </div>
    <div class="resetColumnSet"></div>

  </div><!--end footer_container-->
</footer>






</body></html>