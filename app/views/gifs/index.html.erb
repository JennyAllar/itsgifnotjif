<%= link_to 'Add a Gif!', new_gif_path %>

<div id="columns">


  <% @gifs.each do |gif| %>
    <figure>
      <%= link_to (image_tag gif.url), gif_path(gif) %>
      <figcaption><%= gif.name %></figcaption><div class="heart_icon"><%= link_to (image_tag("heart.png")) %></div>
      
    </figure>
  <% end %>

</div>